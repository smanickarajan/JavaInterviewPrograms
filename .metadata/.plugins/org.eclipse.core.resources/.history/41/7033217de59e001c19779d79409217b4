package com.corejava.interviewquestions;

public class SwapIntwithoutVariable {

	public static void main(String[] args) {
		int x=5;
		int y=10;
		x=x+y;
		y=x-y;
		x=x-y;
		System.out.println("x :"+x);
		System.out.println("y :"+y);

		
		
		int a=5;
		int b=10;
		a=a*b;
		b=a/b;
		a=a/b;
		System.out.println("a :"+a);
		System.out.println("b :"+b);
	}

}
